{
  "manifest_version": 3,
  "name": "Prep",
  "version": "1.0.0",
  "description": "Prepping for SF Hacks",
  "icons": {
    "128": "sf_hacks_sfsu_logo.jpg"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "storage", "notifications"],
  "host_permissions": [
    "<all_urls>",
    "http://localhost:8000/*",
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
